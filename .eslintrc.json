{
    "extends": "standard",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "standard"
    ],
    "rules": {
        "strict": 0,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "jsx-quotes": [2, "prefer-double"],
        "semi": [2, "always"],
        "indent": [2, 4, {"SwitchCase": 1}],
        "brace-style": [2, "stroustrup"],
        "operator-linebreak": [2, "after"]
    }
}